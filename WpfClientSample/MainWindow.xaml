<Window x:Class="WpfClientSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfClientSample"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="BtnOAuthTest" Click="BtnOAuthTest_OnClick">OAuth Login Test</Button>
        <Button x:Name="BtnPasswordTest" Click="BtnPasswordTest_OnClick">Password Login Test</Button>
        <Button x:Name="BtnPlayerTest" Click="BtnPlayerTest_Click">Player Test</Button>
    </StackPanel>
</Window>
